<%- include('./partials/head')%>

<table>
  <tr>
    <th>Movie</th>
    <th>Released Year</th>
    <th>Genre</th>
    <th>Action</th>
  </tr>
  
  <%movies.forEach(movie => { %>
    
    <tr>
      <td><%=movie.name%></td>
      <td><%=movie.released_year%></td>
      <td><%=movie.genre%></td>
      <td><a href="/movies/<%=movie.id%>/addCastRole">Add Cast Role</a></td>
    </tr>
    
  <%})%>
    
</table>

<%- include('./partials/foot')%>