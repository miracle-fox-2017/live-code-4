<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Casts</title>
  </head>
  <body>
    <h2>Casts</h2>
    <table border="1">
      <tr>
        <th>Fullname</th>
        <th>Birth Year</th>
        <th>Gender</th>
      </tr>
      <% for(let i = 0; i < dataCast.length; i++){ %>
        <tr>
          <td><%= dataCast[i].getFullName() %></td>
          <td><%= dataCast[i].birth_year %></td>
          <td><%= dataCast[i].gender %></td>
          <td><a href="/casts/edit/<%= dataCast[i].id %>">Edit</a></td>
          <td><a href="/casts/delete/<%= dataCast[i].id %>">Delete</a></td>
        </tr>
      <% } %>
    </table>
  </body>
</html>